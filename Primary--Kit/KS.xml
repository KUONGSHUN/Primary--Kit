<category id="catNova" name="KS" colour="40">

<!--Arduino引脚-->
  <category id="catNovaBase" name="Arduino--IO" colour="#3EDAFF">

	<block type="Baron_digital_write">
          <value name="PIN">    <!-- 管脚选择 -->
              <shadow type="pins_digital"></shadow>
          </value>
          <value name="STAT">   <!-- 状态选择 -->
            <shadow type="inout_highlow"></shadow>
          </value>
        </block>
	<block type="Baron_digital_read">
        <value name="PIN">    <!-- 管脚选择 -->
              <shadow type="pins_digital"></shadow>
          </value>
          <value name="STAT">   <!-- 状态选择 -->
            <shadow type="inout_highlow"></shadow>
          </value>
        </block>
  <block type="Baron_analog_write">
       <value name="PIN">
              <shadow type="pins_pwm"></shadow>
          </value>
          <value name="NUM">
              <shadow type="math_number">
                  <field name="NUM">0</field></shadow>
          </value>
        </block>

	<block type="Baron_analog_read">
    <value name="PIN">
              <shadow type="pins_analog"></shadow>
          </value>
  </block>
</category>


